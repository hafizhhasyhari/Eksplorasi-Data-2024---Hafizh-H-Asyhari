   "source": [
    "class StandardizeBirthDay():    \n",
    "    def standarisasi(df):\n",
    "        import datetime as dd\n",
    "        \n",
    "        if df['pattern'] == 'slash':\n",
    "            s = df['birthday'].split('/')\n",
    "            try:\n",
    "                day = int(s[0])\n",
    "                month = int(s[1])\n",
    "                year = int(s[2])\n",
    "                if (day >= 1  & day <= 31):\n",
    "                    if (month >= 1  & month <= 12):\n",
    "                        if len(str(year)) == 4:\n",
    "                            return dd.date(year, month, day)\n",
    "                        if len(str(year)) == 2:\n",
    "                            fullyear = int('19'+str(year))\n",
    "                            return dd.date(fullyear, month, day)\n",
    "            except:\n",
    "                return ''\n",
    "        \n",
    "        if df['pattern'] == 'hiphen':\n",
    "            s = df['birthday'].split('-')\n",
    "            try:\n",
    "                day = int(s[0])\n",
    "                month = int(s[1])\n",
    "                year = int(s[2])\n",
    "                if (day >= 1  & day <= 31):\n",
    "                    if (month >= 1  & month <= 12):\n",
    "                        if len(str(year)) == 4:\n",
    "                            return dd.date(year, month, day)\n",
    "                        if len(str(year)) == 2:\n",
    "                            fullyear = int('19'+str(year))\n",
    "                            return dd.date(fullyear, month, day)\n",
    "            except:\n",
    "                return ''\n",
    "            \n",
    "        if df['pattern'] == 'merge':\n",
    "            s = df['birthday']\n",
    "            if len(s) == 8:\n",
    "                try:\n",
    "                    day = int(s[0:2])\n",
    "                    month = int(s[2:4])\n",
    "                    year = int(s[4:8])\n",
    "                    if (day >= 1  & day <= 31):\n",
    "                        if (month >= 1  & month <= 12):\n",
    "                            if len(str(year)) == 4:\n",
    "                                return dd.date(year, month, day)\n",
    "                except:\n",
    "                    return ''\n",
    "            if len(s) == 6 :\n",
    "                try:\n",
    "                    day = int(s[0:2])\n",
    "                    month = int(s[2:4])\n",
    "                    year = int(s[4:6])\n",
    "                    if (day >= 1  & day <= 31):\n",
    "                        if (month >= 1  & month <= 12):\n",
    "                            if len(str(year)) == 2:\n",
    "                                fullyear = int('19'+str(year))\n",
    "                                return dd.date(fullyear, month, day)\n",
    "                except:\n",
    "                    return ''\n",
    "                \n",
    "        if df['pattern'] == 'space':\n",
    "            try:\n",
    "                res = df['birthday'].lower().split()\n",
    "                day = int(res[0])\n",
    "                month = int(df['month_number'])\n",
    "                year = int(res[2])\n",
    "                if len(str(year)) == 4:\n",
    "                    return dd.date(year, month, day)\n",
    "            except:\n",
    "                return ''\n",
    "    \n",
    "    def getMonthNumber(df):\n",
    "        import datetime as dd\n",
    "        \n",
    "        day = []\n",
    "        month = []\n",
    "        year = []\n",
    "        \n",
    "        if df['pattern'] == 'space':\n",
    "            s = df['birthday'].lower()\n",
    "            s1 = s.split()[1]\n",
    "            if s1.find('jan') != -1:\n",
    "                month.append(1)\n",
    "            if (s1.find('feb') != -1) or (s1.find('pebr') != -1) :\n",
    "                month.append(2)\n",
    "            if s1.find('mar') != -1:\n",
    "                month.append(3)\n",
    "            if s1.find('apr') != -1:\n",
    "                month.append(4)\n",
    "            if (s1.find('mei') != -1) or (s1.find('may') != -1):\n",
    "                month.append(5)\n",
    "            if s1.find('jun') != -1:\n",
    "                month.append(6)\n",
    "            if s1.find('jul') != -1:\n",
    "                month.append(7)\n",
    "            if (s1.find('agu') != -1) or (s1.find('aug') != -1):\n",
    "                month.append(8)\n",
    "            if s1.find('sep') != -1:\n",
    "                month.append(9)\n",
    "            if (s1.find('oct') != -1) or (s1.find('okt') != -1):\n",
    "                month.append(10)\n",
    "            if (s1.find('nov') != -1) or (s1.find('nop') != -1):\n",
    "                month.append(11)\n",
    "            if (s1.find('dec') != -1) or (s1.find('des') != -1):\n",
    "                month.append(12)\n",
    "            else:\n",
    "                try:\n",
    "                    month.append(int(s1))\n",
    "                except:\n",
    "                    pass\n",
    "        try:\n",
    "            return int(month[0])\n",
    "        except:\n",
    "            pass\n",
    "    \n",
    "    def isValid(val):\n",
    "        if val != '':\n",
    "            return 'valid'\n",
    "        else:\n",
    "            return 'not-valid'"
   ]
