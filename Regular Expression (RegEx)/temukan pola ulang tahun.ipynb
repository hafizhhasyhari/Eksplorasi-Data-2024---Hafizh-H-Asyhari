   "outputs": [],
   "source": [
    "def getVariation(val):\n",
    "    import re\n",
    "    \n",
    "    if (re.match(r'\\d+/+\\d+/+\\d',val)) :\n",
    "        return 'slash'\n",
    "    if re.match(r'\\d+-+\\d+-\\d',val):\n",
    "        return 'hiphen'\n",
    "    if re.match(r'\\w+\\s+\\w+\\s+\\w',val):\n",
    "        return 'space'\n",
    "    if re.match(r'\\w\\w\\w\\w\\w',val):\n",
    "        return 'merge'\n",
    "    else:\n",
    "        return 'other'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "ls_variasi = [[k,getVariation(k)] for k in text]"
   ]
  },
  {
